{
  "name": "@changelog-test/ui",
  "private": true,
  "author": "Irina Illustrova",
  "description": "UI repo mock for testing changelog generation",
  "main": "src/index.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "postpublish": "git push --follow-tags",
    "release": "release-it --ci"
  },
  "release-it": {
    "git": {
      "tagName": "v${version}",
      "commitMessage": "chore(release): ${version}",
      "requireCleanWorkingDir": false,
      "requireUpstream": false,
      "requireCommits": true
    },

    "hooks": {
      "before:bump": "if ! npx should-semantic-release ; then exit 1 ; fi"
    }
  },
  "license": "ISC",
  "dependencies": {
    "classnames": "2.3.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "version": "0.2.4"
}
