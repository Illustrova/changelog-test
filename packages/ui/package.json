{
  "name": "@changelog-test/ui",
  "private": true,
  "author": "Irina Illustrova",
  "description": "UI repo mock for testing changelog generation",
  "main": "src/index.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "postpublish": "git push --follow-tags",
    "release": "release-it"
  },
  "release-it": {
    "git": false,
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        },
        "infile": "CHANGELOG.md"
      }
    }
  },
  "license": "ISC",
  "dependencies": {
    "classnames": "2.3.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "version": "0.2.0"
}
